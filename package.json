{
  "name": "wedeo",
  "version": "0.0.1",
  "main": "function.js",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "prev": "next dev -p 3001",
    "build": "next build",
    "deploy": "firebase deploy --only functions,hosting:app",
    "deploy:hosting": "firebase deploy --only hosting:app",
    "deploy:functions": "cd ./functions && npm run deploy",
